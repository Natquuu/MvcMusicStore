@model IEnumerable<MvcMusicStore.Models.Genre>

@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count()
    genres:</p>
<u1>
    @foreach (var genre in Model)
    {
        <li>@Html.ActionLink(genre.Name,
           "Browse", new { genre = genre.Name})</li>
    }
</u1>
